	"scripts": {
		"install:terminal": "cd terminal && CXXFLAGS='--std=c++17' npm install",
		"color": "node color.js",
		"start": "cross-env NODE_ENV=dev node --max-old-space-size=4096 server.js --history-api-fallback --inline  --history-api-fallback --watch",
		"dll": "cross-env NODE_ENV=pro webpack --config webpack/webpack.dll.config.js",
		"build:dll": "cross-env NODE_ENV=pro webpack --config webpack/webpack.pro.config.js",
		"build:plugs": "cross-env NODE_ENV=pro webpack --config webpack/webpack.plugs.config.js",
		"build:pro": "npm-run-all color dll build:dll",
		"build:all": "cross-env NODE_PLUG=true npm-run-all build:plugs build:pro",
		"build": "npm-run-all build:pro",
		"start:open": "cross-env NODE_ENV=dev NODE_OPEN=true node server.js --history-api-fallback --inline  --history-api-fallback",
		"build:on": "cross-env NODE_ENV=pro NODE_OPEN=true webpack --config webpack/webpack.open.config.js",
		"build:open": "npm-run-all color dll build:on",
		"build:source": "cross-env NODE_ENV=pro webpack --config webpack/webpack.source.config.js",
		"test": "echo \"Error: no test specified\" && exit 1"
