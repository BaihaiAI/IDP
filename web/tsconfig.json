{
    "compilerOptions": {
        "experimentalDecorators": true,
        "target": "ESNext",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "importHelpers": true,
        "sourceMap": true,
        "baseUrl": "./",
        "outDir": "./dist/",
        "noImplicitAny": false,
        "noUnusedParameters": false,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@/assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@idp/*": [
                "src/idp/*"
            ],
            "idpStudio/*": [
                "src/*"
            ],
            "idpServices/*": [
                "src/services/*"
            ],
            "idpUtils/*": [
                "src/utils/*"
            ],
            "idpStore/*": [
                "src/store/*"
            ],
            "idp/*": [
                "src/idp/*"
            ]
        }
    },
    "typeRoots": [
        "typings.d.ts",
    ],
    "awesomeTypescriptLoaderOptions": {
        /* ... */
    },
    "include": [
        "src/**/*",
        "config/**/*",
        "typings.d.ts",
        "extensions/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}